# Variables d'environnement pour la production
# Copier ce fichier vers .env.prod et configurer avec vos vraies valeurs

# Configuration Docker
DOCKER_USERNAME=katekate7
TAG=latest

# Configuration Base de données
MYSQL_ROOT_PASSWORD=your_secure_mysql_root_password
MYSQL_DATABASE=bank_prod
MYSQL_USER=bank_user
MYSQL_PASSWORD=your_secure_mysql_password

# Configuration Symfony
APP_SECRET=your_secure_app_secret_key_32_chars_min
JWT_SECRET=your_secure_jwt_secret_key
JWT_PASSPHRASE=your_secure_jwt_passphrase

# URLs de production (à adapter selon votre domaine)
FRONTEND_URL=https://bank.example.com
BACKEND_URL=https://api.bank.example.com
